{
  "name": "webpack-config-assign",
  "version": "1.0.0-beta",
  "description": "A utility to recursively merge webpack config(s)",
  "main": "lib/index.js",
  "esnext:main": "src/index.js",
  "scripts": {
    "prebuild": "del \"lib/**/*\" & npm run lint",
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "coverage": "npm run test -- --coverage --no-cache",
    "lint": "eslint \"src/**/*.js\"",
    "prepublishOnly": "npm run build",
    "test": "cross-env NODE_ENV=test jest"
  },
  "keywords": [
    "webpack",
    "assign",
    "extend",
    "utility"
  ],
  "author": "Dustin Schau <dustinschau@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "is-equal-regex": "~0.1.0",
    "lodash.clone": "~4.5.0",
    "object-assign": "~4.1.0"
  },
  "devDependencies": {
    "babel-cli": "~6.18.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-latest": "~6.16.0",
    "babel-register": "~6.18.0",
    "coveralls": "~2.11.15",
    "cross-env": "~3.1.3",
    "del-cli": "~0.2.0",
    "eslint": "~3.10.2",
    "eslint-config-airbnb-base": "~10.0.1",
    "eslint-plugin-import": "~2.2.0",
    "jest": "~17.0.3"
  },
  "jest": {
    "testRegex": "/test/.*test\\.js$"
  }
}
